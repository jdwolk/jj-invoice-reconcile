<!DOCTYPE html>
<html>
<head>
  <title>JJInvoiceReconcile</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="bs-component">
    <nav class="navbar navbar-default">
      <div class="container">
        <div class="navbar-header">
          <%= link_to "J&J", admin_db_uploads_path, class: "navbar-brand" %>
        </div>
        <div class="navbar-collapse collapse">
          <ul class="nav navbar-nav">
            <li><%= link_to 'DB Upload', admin_db_uploads_path %></li>
            <li><%= link_to 'Reconcile Orders', admin_orders_path %></li>
          </ul>
        </div>
      </div>
    </nav>
  </div>
  <div class="container">
    <%= yield %>
  </div>
</body>
</html>
